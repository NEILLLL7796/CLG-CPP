#include<iostream>
using namespace std;
int main()
{
    struct 
    {
        string NAME;
        double ROLL;
        double PRN;
        
    } myfriend1, myfriend2;

    // FRIEND INFO 1
    myfriend1.NAME = "NEIL"; 
    myfriend1.ROLL = 2271;
    myfriend1.PRN = 169;

    // FRIEND INFO 2
    myfriend2.NAME = "ELVIS"; 
    myfriend2.ROLL = 2296;
    myfriend2.PRN = 196;

    // PRINT THE INFO
    cout << myfriend1.NAME << " " << myfriend1.ROLL << " " << myfriend1.PRN << endl;
    cout << myfriend2.NAME << " " << myfriend2.ROLL << " " << myfriend2.PRN << endl;
}
